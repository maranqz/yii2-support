{
  "name": "maranqz/yii2-ssupport",
  "description": "yii2 realization of simple support interface",
  "type": "yii2-extension",
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.2 <8",
    "psr/event-dispatcher": "^1.0",
    "yiisoft/yii2-dev": "2.*",
    "la-haute-societe/yii2-save-relations-behavior": "^1.7",
    "league/flysystem": "^1.0",
    "symfony/event-dispatcher": "v4.*",
    "kartik-v/yii2-date-range": "^1.7@dev",
    "paulzi/yii2-json-behavior": "dev-master"
  },
  "replace": {
    "maranqz/support-core-component": "self.version",
    "maranqz/support-referee-component": "self.version"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "2.*"
  },
  "scripts": {
    "cs-fixer": "php-cs-fixer fix -vvv --config=./.php_cs.dist"
  },
  "autoload": {
    "psr-4": {
      "SSupport\\": "src/"
    }
  },
  "extra": {
    "bootstrap": "SSupport\\Bootstrap"
  }
}
