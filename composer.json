{
  "name": "maranqz/yii2-support",
  "description": "yii2 realization of support interface",
  "type": "project",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.2 <8",
    "doctrine/collections": "1.*",
    "psr/event-dispatcher": "^1.0",
    "yiisoft/yii2-dev": "2.*"
  },
  "replace": {
    "maranqz/support-core-component": "self.version",
    "maranqz/support-referee-component": "self.version"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "2.*"
  },
  "scripts": {
    "cs-fixer": "php-cs-fixer fix -v --config=./.php_cs.dist"
  },
  "autoload": {
    "psr-4": {
      "Support\\": "src/"
    }
  }
}
